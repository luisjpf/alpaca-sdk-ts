{
  "name": "@luisjpf/alpaca-sdk",
  "version": "0.3.3",
  "description": "Complete Alpaca SDK - Trading, Broker, Market Data, and Streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate:types": "tsx scripts/generate-types.ts"
  },
  "keywords": [
    "alpaca",
    "trading",
    "api",
    "sdk",
    "stocks",
    "crypto",
    "options",
    "websocket",
    "typescript",
    "broker",
    "market-data",
    "streaming"
  ],
  "author": "Luis JP Fleming <luis.j245@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisjpf/alpaca-sdk-ts.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "openapi-fetch": "^0.13.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.17.0",
    "msw": "^2.7.0",
    "openapi-typescript": "^7.4.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
