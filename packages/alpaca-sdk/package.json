{
  "name": "@alpaca-sdk/alpaca-sdk",
  "version": "0.1.0",
  "description": "Complete Alpaca SDK - Trading, Broker, Market Data, and Streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["alpaca", "sdk", "trading", "broker", "market-data", "streaming"],
  "author": "Luis JP Fleming <luis.j245@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@alpaca-sdk/core": "workspace:*",
    "@alpaca-sdk/trading": "workspace:*",
    "@alpaca-sdk/broker": "workspace:*",
    "@alpaca-sdk/market-data": "workspace:*",
    "@alpaca-sdk/streaming": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
