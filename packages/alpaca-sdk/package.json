{
  "name": "@luisjpf/alpaca-sdk",
  "version": "0.2.2",
  "description": "Complete Alpaca SDK - Trading, Broker, Market Data, and Streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "alpaca",
    "sdk",
    "trading",
    "broker",
    "market-data",
    "streaming"
  ],
  "author": "Luis JP Fleming <luis.j245@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.0",
    "@msgpack/msgpack": "^3.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@luisjpf/core": "workspace:*",
    "@luisjpf/trading": "workspace:*",
    "@luisjpf/broker": "workspace:*",
    "@luisjpf/market-data": "workspace:*",
    "@luisjpf/streaming": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
