{
  "name": "alpaca-sdk-ts",
  "version": "0.0.0",
  "private": true,
  "description": "Modern TypeScript SDK for Alpaca Trading, Broker, and Market Data APIs",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "generate:types": "tsx scripts/generate-types.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.17.0",
    "msw": "^2.7.0",
    "openapi-fetch": "^0.13.0",
    "openapi-typescript": "^7.4.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "turbo": "^2.3.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "alpaca",
    "trading",
    "api",
    "sdk",
    "stocks",
    "crypto",
    "options",
    "websocket",
    "typescript"
  ],
  "author": "Luis JP Fleming <luis.j245@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisjp/alpaca-sdk-ts.git"
  }
}
